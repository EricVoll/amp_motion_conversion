<?xml version="1.0"?>
<robot  xmlns:xacro="http://www.ros.org/wiki/xacro"  name="dumpUrdf">

<xacro:macro name="ball_joint" params="parent child *origin">

  <joint name="${parent}_${child}_joint1" type="continuous">
	<xacro:insert_block name="origin"/>
    <parent link="${parent}"/>
    <child link="${parent}_${child}_link1"/>
    <axis xyz="1 0 0" />
  </joint>

  <link name="${parent}_${child}_link1">
    <inertial>
      <mass value="5e-4" />
      <inertia ixx="5e-4" ixy="0" ixz="0" iyy="5e-5" iyz="0" izz="5e-4" />
    </inertial>
  </link>

  <joint name="${parent}_${child}_joint2" type="continuous">
    <parent link="${parent}_${child}_link1"/>
    <child link="${parent}_${child}_link2"/>
    <axis xyz="0 1 0" />
  </joint>

  <link name="${parent}_${child}_link2">
    <inertial>
      <mass value="5e-4" />
      <inertia ixx="5e-4" ixy="0" ixz="0" iyy="5e-5" iyz="0" izz="5e-4" />
    </inertial>  
  </link>

  <joint name="${parent}_${child}_joint3" type="continuous">
    <parent link="${parent}_${child}_link2"/>
    <child link="${parent}_${child}_link3"/>
    <axis xyz="0 0 1" />
  </joint>

  <link name="${parent}_${child}_link3">
    <inertial>
      <mass value="5e-4" />
      <inertia ixx="5e-4" ixy="0" ixz="0" iyy="5e-5" iyz="0" izz="5e-4" />
    </inertial>  
  </link>

  <joint name="${parent}_${child}_joint4" type="fixed">
    <parent link="${parent}_${child}_link3"/>
    <child link="${child}"/>
  </joint>

</xacro:macro>

<xacro:macro name="sphere_v_c" params="radius *origin">
	<collision>
		<xacro:insert_block name="origin"/>
		<geometry>
			<sphere radius = "${radius}"/>
		</geometry>
	</collision>
	<visual>
		<geometry>
			<sphere radius="${radius}"/>
		</geometry>
	</visual>
</xacro:macro>

<xacro:macro name="cylinder_v_c" params="radius length *origin">
	<collision>
		<xacro:insert_block name="origin"/>
		<geometry>
			<cylinder length="${length}" radius = "${radius}"/>
		</geometry>
	</collision>
	<visual>
		<xacro:insert_block name="origin"/>
		<geometry>
			<cylinder length="${length}" radius="${radius}"/>
		</geometry>
	</visual>
</xacro:macro>

